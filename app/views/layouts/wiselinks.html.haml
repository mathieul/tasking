- wiselinks_title page_title
%body
  = render "layouts/navigation_bar"
  %header.title
    .container
      .row
        %div{page_width(:title)}
          %h1= page_title
        %div{page_width(:description)}
          = yield(:page_description)

  %div{page_container}
    %br

    = flash_messages

    = yield

  = yield :javascript
