- page_title "Sprint Tasking"
- page_description do
  %table.table.table-bordered.table-condensed
    %thead
      %tr
        %th Number
        %th From
        %th To
        %th Velocity
        %th Status
    %tbody
      %tr
        %td= @sprint.label
        %td= @sprint.start_date
        %td= @sprint.end_date
        %td= @sprint.projected_velocity
        %td= @sprint.status

- content_for :javascript do
  = javascript_tag "App.initSprints({});"

%p TODO
